generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Users {
  id            String    @id @default(cuid()) // cuid : cjld2cjxh0000qzrmn831i7rn 
  first_name    String?
  last_name     String?
  email         String    @unique
  phone_number  String
  password      String
  date_of_birth DateTime?
  gender        String?
  image         String?
  address       String?
  role          String?
  created_at    DateTime  @default(now())
  updated_at    DateTime?
}
